html {
	margin: 0;
	padding: 0;
}

.main, .topBar{
	max-width: 1000px;
	background-color: transparent;
}
.main{
	overflow:hidden;
	}
	
body, .topBar {
	background-color: #FFFBD7;
}

.main .contentItem {
	background-color: #224422;
	overflow: hidden;
	color: #FFFBD7;
}

.main .contentItem a {
	color: #ff00ff;
	text-decoration: none;
}

.navigation a, .links a, .authorMark a {
	background-color: #224422;
	text-decoration: none;
	color: #FFFBD7;
	font-weight: bold;
}

.navigation a p, .links a p, .authorMark a p {
	text-align: center;
}

.navigation a:hover, .navigation .active, .links a:hover, .authorMark a:hover, .authorMark .active{
	background-color: #339933;
	color: #FFFBD7;
}

.linkImg {
	background-color: #FFFBD7;
}

.authorMark{
	font-size: 10px;
	background-color: transparent;
}

.banner a{
	text-decoration: none;
}

.bannerGraphic {
	color: #006600;
}
.bannerGraphic:hover {
	color: #339933;
}